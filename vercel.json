{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    {
      "src": "/shorten", 
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/red/history", 
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)", 
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}
